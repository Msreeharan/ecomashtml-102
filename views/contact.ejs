    <style>
        .contact-info {
            background: #fff;
            border-radius: 16px;
            padding: 30px 25px;
            box-shadow: 0 5px 20px rgba(0, 0, 0, 0.05);
            position: relative;
            text-align: center;
            transition: all 0.3s ease-in-out;
            overflow: hidden;
        }
    
        /* Orange corners */
        .contact-info::before,
        .contact-info::after {
            content: '';
            position: absolute;
            width: 40px;
            height: 40px;
            border: 3px solid #000cf5;
        }
    
        .contact-info::before {
            top: 0;
            right: 0;
            border-left: none;
            border-bottom: none;
            border-top-right-radius: 10px;
        }
    
        .contact-info::after {
            bottom: 0;
            left: 0;
            border-top: none;
            border-right: none;
            border-bottom-left-radius: 10px;
        }
    
        .contact-info-icon {
            width: 70px;
            height: 70px;
            background: #0400f5;
            color: white;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 30px;
            margin: 0 auto 20px;
        }
    
        .contact-info h5 {
            font-size: 18px;
            font-weight: 600;
            margin-bottom: 10px;
        }
    
        .contact-info p {
            font-size: 14px;
            color: #333;
            margin: 0;
            line-height: 1.6;
        }
    
        .contact-info a {
            color: #333;
            text-decoration: none;
            font-size: 14px;
        }
    
        @media (max-width: 767px) {
            .contact-info {
                margin-bottom: 30px;
            }
        }
        
